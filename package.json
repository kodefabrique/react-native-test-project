{
  "name": "Test Project",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "postinstall": "npx jetify",
    "postinstallPatchRN": "patch-package",
    "postversion": "react-native-version",
    "start": "npx react-native start",
    "ios:start": "npx react-native run-ios",
    "ios:asset": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ./ios",
    "android:asset": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:build": "cd android && ./gradlew assembleDebug",
    "android:start": "npx react-native run-android",
    "codegen:widget": "node ./scripts/cli_snippets",
    "typescript:check": "npx tsc --skipLibCheck --noEmit",
    "prettier:write": "npx prettier --write index.js app",
    "prettier:check": "npx prettier --check index.js app",
    "eslint:check": "npx eslint --ext .tsx app",
    "jest:test": "npx jest test ./tests",
    "scripts:i18n": "node ./scripts/translation_build && npx prettier --write ./assets/i18n",
    "project:clean": "npx react-native clean-project-auto",
    "replace:logo": "node ./scripts/logo_replace",
    "replace:name": "node ./scripts/display_replace && npx prettier --write app.json",
    "replace:launch": "node ./scripts/launch_replace",
    "publish:check": "npm run prettier:check && npm run eslint:check && npm run typescript:check",
    "publish:replace": "npm run replace:logo && npm run replace:name && npm run replace:launch",
    "publish:build/android": "npm run publish:replace && npm run android:asset",
    "publish:build/ios": "npm run publish:replace && npm run ios:asset"
  },
